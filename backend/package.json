{
  "name": "techmap-backend",
  "version": "1.0.0",
  "description": "Backend API for TechMap France",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "migrate": "ts-node src/infrastructure/database/migrate.ts",
    "fetch-jobs": "ts-node src/scripts/fetchJobs.ts"
