{
  "name": "techmap-backend",
  "version": "1.0.0",
  "description": "Backend API for TechMap France",
  "main": "dist/server.js",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "lint": "eslint src",
    "fetch-jobs": "ts-node src/scripts/fetchJobs.ts",
    "migrate": "ts-node scripts/migrate.ts",
    "seed": "ts-node scripts/seed.ts",
    "db:reset": "npm run migrate && npm run seed",
    "db:setup": "npm run db:reset"
  },
  "keywords": [
    "job-board",
    "analytics",
    "france"
  ],
  "author": "Achref Gataa",
  "license": "MIT",
  "dependencies": {
    "@upstash/redis": "^1.35.6",
    "axios": "^1.13.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-validator": "^7.3.0",
    "helmet": "^8.1.0",
    "node-cron": "^4.2.1",
    "pg": "^8.16.3",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@jest/globals": "^30.2.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.1",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.15.6",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.38.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
